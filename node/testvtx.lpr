  PrintLn(RowColor(ANSI_BLUE, ANSI_BLACK, HorzGrad) + RowSize(150)
    + ' Sprites');
  PrintLn;

  // clear all and define a single test sprite.
  Print(#27'[0;0_'); // clear all
  //				 +------------- sprite commands
  //				 | +----------- sprite define
  //				 | | +--------- number: 1
  //				 | | | +------- type: raw svg
  //				 | | | |        hex3 data
  //				 | | | |        |
	Print(#27'[0;0;1;2;' + testsvg + '_');

  PrintLn(#27'[94mSprites are small SVG clipart sent from the server to the client. This');
  PrintLn('is done in two steps. The first step is to define the sprite on the client.');
  PrintLn('The second is to display a sprite on the page.');
  PrintLn;
  println(#27'[93mCSI 0;0;'#27'[3mn;t;d'#27'[23m _ '#27'[94m: Define sprite n (1-64) defined as d of type t.');
  println('    t=type: 0=url:unicode encoded characters, 1=UTF8 hex3 url, 2=raw UTF8 hex3');
  println('    svg, 3=raw UTF8 hex3 svg deflated, 4=raw UTF8 hex3 svg Base64.');
  PrintLn;
  PrintLn('Once a sprite has been defined, it can be displayed on the page using:');
  PrintLn;
  PrintLn(#27'[93mCSI 0;1;'#27'[3mn;s;w;h;z'#27'[23m _ '#27'[94m: Display a sprite number n (1-64) defined as s at the');
  PrintLn('    current cursor position with size w and h (measured in default character');
  PrintLn('    sizes, A width of 1 equals 1 character width at default size, A height of');
  PrintLn('    1 equals 1 character height). Z = 0 if the sprite is to appear under the');
  PrintLn('    text, 1 if on top.');
  PrintLn(#27'[93mCSI 0;1;'#27'[3mn'#27'[23m _ '#27'[94m: Remove sprite n.');
  PrintLn(#27'[93mCSI 0;1 _ '#27'[94m: Remove all sprites.');
  PrintLn(#27'[93mCSI 0;2;z;r;c _ '#27'[94m: Move sprite s to new r,c in t time (ms).');
  PrintLn(#27'[93mCSI 0;3;s;z _ '#27'[94m: Move sprite s to new Z.');
  PrintLn;

  Print(SGR(ANSI_GREEN) + 'Press ' + SGR(ANSI_YELLOW) + HOTSPOT(5,1,0,' ') + 'Space'
    + SGR(ANSI_GREEN)+' to randomly display a sprite, '+HOTSPOT(3,1,0,'Q')+'['
    + SGR(ANSI_YELLOW) + 'Q' + SGR(ANSI_GREEN) + ']uit when done: ');

  firstin := true;
  mode := 0;
  repeat
    key := upCase(GetKey);
    if key = ' ' then
    begin
      if firstin then
      begin
        r := 0;
        c := 0;
        w := 8;
        h := 4;
        z := 1;
        firstin := false;
      end
      else
      begin
        r := random(20) + 1;
        c := random(80 - w) + 1;
        w := random(16) + 3;
        h := random(10) + 3;
        z := random(2);
      end;

      if mode = 0 then
      begin
	      Print(CSI + 's' + MOVETO(r,c) + CSI + '0;1;1;1;' + inttostr(w) + ';'
  	      + inttostr(h) + ';' + inttostr(z) + '_' + CSI + 'u');
      	mode := 1;
      end
      else
      begin
        Print(CSI + '0;2;1;' + inttostr(r) + ';' + inttostr(c) + ';500_');
      	mode := 0;
      end;

    end;
  until key = 'Q';
  Print(CSI + '0;1_' + CSI + '0;0_');
end;

procedure Page8;
var
  key : string;
	filenames : array [0..9] of string = (
		'ansi0.5.utf',
		'ansi0.utf',
		'ansi1.utf',
		'ansi2.utf',
		'ansi3.utf',
		'ansi4.utf',
		'ansi5.utf',
		'ansi6.utf',
		'ansi7.utf',
		'ansi8.utf'
  );
  i : integer;

begin
  Print(VTXMODE + HOME + CLS + SGR(ANSI_LTCYAN, [SGR_RESET]));
  i := 0;

  repeat

    Println(#27'[c' + BBSMODE);	// clear & reset
    Print(#27'[1;9*r'#27'[0m'#27'[2J'#27'[H');

		PrintANSI(filenames[i]);
    inc(i); if i >= length(filenames) then i := 0;

    Print(CRLF + #27'[0m' + SGR(ANSI_GREEN) + 'Press SPACE for next or '
      + HOTSPOT(3,1,0,'Q') + '[' + SGR(ANSI_YELLOW) + 'Q' + SGR(ANSI_GREEN)
      + ']uit when done: ');

		repeat
    	key := upCase(GetKey);
    until (key = 'Q') or (key = ' ');
  until key = 'Q';

  Print(CLS + #27'[c');
  Print(VTXMODE);
  Print(#27'[1;0*r');
end;



