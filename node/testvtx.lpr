procedure PrintTeletext(filename:string);
var
  fin : TFileStream;
  len : integer;
  buff : pbyte;
begin
  if fileexists(filename) then
  begin
	  fin := TFileStream.Create(filename, fmOpenRead);
	  len := fin.Size;
	  buff := GetMemory(len);
	  fin.ReadBuffer(buff[0], len);
	  fin.Free;
	  Print(CSI + '?51h');
	  PrintBin(buff, len);
	  print(ESC);
  	freememory(buff);
  end;
end;

